{% extends 'base.html.twig' %}
{% block body %}

    <form action="{{ path('new-user') }}" method="post" enctype="multipart/form-data">
        <p>Логин: <input type="text" name="login"></p>
        <p>Пароль: <input type="text" name="password"></p>
        <p>Имя: <input type="text" name="name"></p>
        <p>Аватар: <input type="file" name="user_avatar"></p>
        <p><input type="submit" name="save" value="Зарегестрировать"></p>
    </form>

{% endblock %}